# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_CLIENT_ID=your_discord_client_id_here
DISCORD_GUILD_ID=your_guild_id_here

# Neon PostgreSQL Database URL
DATABASE_URL=postgresql://username:password@ep-xxx-xxx-xxx.us-east-2.aws.neon.tech/neondb?sslmode=require

# Timezone (default: Asia/Jakarta for WIB)
TIMEZONE=Asia/Jakarta

# 5AM Club Channel ID (required - /present only works in this channel)
FIVEAM_CHANNEL_ID=your_5am_club_channel_id_here

# Channel ID for leaderboard announcements (optional - defaults to FIVEAM_CHANNEL_ID)
LEADERBOARD_CHANNEL_ID=your_channel_id_here
