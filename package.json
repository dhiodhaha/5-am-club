{
  "name": "5-am-club",
  "version": "1.0.0",
  "description": "Discord bot for 5AM Club presence tracking",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "deploy-commands": "tsx src/deploy-commands.ts",
    "migrate": "tsx src/db/migrate.ts",
    "pm2:start": "npm run build && pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop 5am-club-bot",
    "pm2:restart": "pm2 restart 5am-club-bot",
    "pm2:logs": "pm2 logs 5am-club-bot",
    "pm2:status": "pm2 status"
  },
  "keywords": [
    "discord",
    "bot",
    "5am-club",
    "presence"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@neondatabase/serverless": "^0.9.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
}
